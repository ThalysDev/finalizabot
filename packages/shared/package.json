{
  "name": "@finalizabot/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "imports": {
    "#generated/prisma": "./generated/prisma/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./prisma": {
      "types": "./dist/db/prisma.d.ts",
      "import": "./dist/db/prisma.js",
      "require": "./dist/db/prisma.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.cjs"
    },
    "./calc": {
      "types": "./dist/calc/market.d.ts",
      "import": "./dist/calc/market.js",
      "require": "./dist/calc/market.cjs"
    },
    "./generated/prisma": {
      "types": "./generated/prisma/index.d.ts",
      "default": "./generated/prisma/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "6.19.2"
  },
  "devDependencies": {
    "prisma": "6.19.2",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
